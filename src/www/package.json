{
  "name": "www",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next",
    "build": "contentlayer build && pnpm build:registry && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "test": "jest",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "prisma:migrate": "pnpm prisma migrate dev",
    "typecheck": "contentlayer build && tsc --noEmit",
    "format:write": "prettier --write \"**/*.{ts,tsx,mdx}\" --cache",
    "format:check": "prettier --check \"**/*.{ts,tsx,mdx}\" --cache",
    "rss": "node scripts/rss.mjs",
    "sitemap": "node scripts/sitemap.mjs",
    "postbuild": "npm run rss && npm run sitemap"
  },
